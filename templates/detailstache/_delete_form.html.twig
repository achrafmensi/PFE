<form method="post" action="{{ path('detailstache_delete', {'id': detailstache.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ detailstache.id) }}">
    <button class="btn btn-danger" ><i  class="ti-trash"></i></button>
</form>
